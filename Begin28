
public class Begin28 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
double a=4, aux1, aux2;

aux1=a*a;
System.out.println("A^2="+aux1);

aux2=a*aux1;
System.out.println("A^3="+aux2);

aux1=aux1*aux2;
System.out.println("A^5="+aux1);

aux2=aux1*aux1;
System.out.println("A^10="+aux2);

aux1=aux1*aux2;
System.out.println("A^10="+aux1);
	}

}
