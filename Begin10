public class Begin10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
float a,b,sum,diff,prod,quot;
a=4;
b=3;
sum=a*a+b*b;
diff=(a+b)*(a-b);
prod=(a*a)*(b*b);
quot=(a*a)/(b*b);
System.out.println("Сумма квадратов равна "+sum);
System.out.println("Разность квадратов равна "+diff);
System.out.println("Произведение квадратов равно "+prod);
System.out.println("Частное квадратов равно "+quot);
	}

}
